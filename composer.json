{
    "name": "abdulaziz96/laravel-quick-login",
    "description": "A Laravel package for quick one-time login links.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "abdulaziz zaid",
            "email": "aziz.sa03@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelQuickLogin\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelQuickLogin\\LaravelQuickLoginServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}

